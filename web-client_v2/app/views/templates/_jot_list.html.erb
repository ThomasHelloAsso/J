          Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.......
        </p>
        <a class="more" href="#">More</a>
      </div>

      <!-- ATTACHMENT LIST -->
      <div class="attach pad_ten">
        <span class="attach-icon">&nbsp;</span>
        <div class="jot_asset_left left">
          <img alt="video" src="/assets/video.jpg">
        </div>
        <div id="video-detail" class="right">

          <span class="multimedia padzero">&nbsp;</span>
          <p class="video-title padzero">This is Dummie Video (live)</p>
          <div class="clear"></div>
          <p class="video-detail">A <strong>Jazz </strong>Evening of Fourplay <br>(Bob James, Lee Ritenour, </p>
          <a class="link" href="#">http://youtube.com/watch?wjfi2i</a>
          <div class="clear"></div>
          <p class="uploaded">uploaded via youtube</p>
          <a class="more" href="#">2 More attachment</a>
        </div>
      </div>






      <# if(tags.length > 0){ #>
      <div class="tag-incognito">
        <span class="hot_jot_incognito_icon">&nbsp;</span>

        <div class="button_tag">
          <ul>
            <# _.each(tags, function(tag){ #>
            <li>
              <span class="button_tagText"><#= tag.name #></span>
              <span class="not_activeSearch"></span>
            </li>


            <# }) #>

            <li>
              <span class="choice_text active_choiceTexts">Dummie</span>
              <span class="activeSearch"></span>
            </li>
          </ul>
          <div class="clear"></div>
        </div>
      </div>
      <# } #>

